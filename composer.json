{
    "name": "hyperfx/feishu",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "飞书SDK",
    "autoload": {
        "psr-4": {
            "HyperfX\\Feishu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "hyperf/di": "^2.0",
        "hyperf/guzzle": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/config": "^2.0",
        "hyperf/testing": "^2.0",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^0.12"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "php vendor/bin/phpunit -c phpunit.xml --colors=always",
        "analyse": "php vendor/bin/phpstan analyse --memory-limit 300M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "HyperfX\\Feishu\\ConfigProvider"
        }
    }
}
